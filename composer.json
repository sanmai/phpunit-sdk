{
    "name": "sanmai/phpunit-sdk",
    "type": "library",
    "description": "PHPUnit SDK",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "jms/serializer": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "infection/infection": "^0.14.2 || ^0.15.0",
        "phan/phan": "^2.4 || ^3.0 || ^4.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.11.19 || ^0.12.0 || ^1.0.0",
        "phpunit/phpunit": "^8.4",
        "vimeo/psalm": "^3.6 || ^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PHPUnitSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\PHPUnitSDK\\": "tests/"
        }
    }
}
